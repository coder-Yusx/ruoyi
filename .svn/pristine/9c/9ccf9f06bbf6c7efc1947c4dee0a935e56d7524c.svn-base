<!DOCTYPE html>
<html lang="zh" xmlns:th="http://www.thymeleaf.org" >
<head>
	<th:block th:include="include :: header('修改附件文件')" />
</head>
<body class="white-bg">
    <div class="wrapper wrapper-content animated fadeInRight ibox-content">
        <form class="form-horizontal m" id="form-filemanagerFile-edit" th:object="${filemanagerFile}">
            <input id="id" name="id" th:field="*{id}"  type="hidden">
			<div class="form-group">
				<label class="col-sm-3 control-label">显示名：</label>
				<div class="col-sm-8">
					<input id="showname" name="showname" th:field="*{showname}" class="form-control" type="text">
				</div>
			</div>
			<div class="form-group">
				<label class="col-sm-3 control-label">FTP路径：</label>
				<div class="col-sm-8">
					<input id="ftppath" name="ftppath" th:field="*{ftppath}" class="form-control" type="text">
				</div>
			</div>
			<div class="form-group">
				<label class="col-sm-3 control-label">备注：</label>
				<div class="col-sm-8">
					<input id="memoa" name="memoa" th:field="*{memoa}" class="form-control" type="text">
				</div>
			</div>

			<div style="display: none">
				<div class="form-group">
					<label class="col-sm-3 control-label">ftp服务：</label>
					<div class="col-sm-8">
						<input id="ftpserver" name="ftpserver" th:field="*{ftpserver}" class="form-control" type="text">
					</div>
				</div>
				<div class="form-group">
					<label class="col-sm-3 control-label">所在目录：</label>
					<div class="col-sm-8">
						<input id="dirid" name="dirid" th:field="*{dirid}" class="form-control" type="text">
					</div>
				</div>
				<div class="form-group">
					<label class="col-sm-3 control-label">扩展名：</label>
					<div class="col-sm-8">
						<input id="extendname" name="extendname" th:field="*{extendname}" class="form-control" type="text">
					</div>
				</div>
				<div class="form-group">
					<label class="col-sm-3 control-label">上传用户：</label>
					<div class="col-sm-8">
						<input id="userid" name="userid" th:field="*{userid}" class="form-control" type="text">
					</div>
				</div>
				<div class="form-group">
					<label class="col-sm-3 control-label">上传时间：</label>
					<div class="col-sm-8">
						<input id="uploaddate" name="uploaddate" th:field="*{uploaddate}" class="form-control" type="text">
					</div>
				</div>
				<div class="form-group">
					<label class="col-sm-3 control-label">业务主键：</label>
					<div class="col-sm-8">
						<input id="xtajbh" name="xtajbh" th:field="*{xtajbh}" class="form-control" type="text">
					</div>
				</div>
				<div class="form-group">
					<label class="col-sm-3 control-label">排序号：</label>
					<div class="col-sm-8">
						<input id="seq" name="seq" th:field="*{seq}" class="form-control" type="text">
					</div>
				</div>
			</div>

			<div class="form-group" style="background-color: red;height: 3px;"></div>
		</form>

		<div class="form-horizontal m" th:object="${filemanagerFile}">
			<div class="form-group">
				<label class="col-sm-3 control-label">接件编号：</label>
				<div class="col-sm-8">
					<input id="jjbh" name="jjbh" th:field="*{jjbh}" class="form-control" type="text">
				</div>
			</div>
			<div class="form-group">
				<label class="col-sm-3 control-label">权利人类型：</label>
				<div class="col-sm-8">
					<input id="qlrlx" name="qlrlx" th:field="*{qlrlx}" class="form-control" type="text">
				</div>
			</div>
			<div class="form-group">
				<label class="col-sm-3 control-label">权利人名称：</label>
				<div class="col-sm-8">
					<input id="qlrmc" name="qlrmc" th:field="*{qlrmc}" class="form-control" type="text">
				</div>
			</div>
			<div class="form-group">
				<label class="col-sm-3 control-label">权利人电话：</label>
				<div class="col-sm-8">
					<input id="qlrdh" name="qlrdh" th:field="*{qlrdh}" class="form-control" type="text">
				</div>
			</div>
			<div class="form-group">
				<label class="col-sm-3 control-label">权利人联系地址：</label>
				<div class="col-sm-8">
					<input id="qlrlxdz" name="qlrlxdz" th:field="*{qlrlxdz}" class="form-control" type="text">
				</div>
			</div>
			<div class="form-group">
				<label class="col-sm-3 control-label">权利人证件名称：</label>
				<div class="col-sm-8">
					<input id="qlrzjmc" name="qlrzjmc" th:field="*{qlrzjmc}" class="form-control" type="text">
				</div>
			</div>
			<div class="form-group">
				<label class="col-sm-3 control-label">权利人证件编码：</label>
				<div class="col-sm-8">
					<input id="qlrzjhm" name="qlrzjhm" th:field="*{qlrzjhm}" class="form-control" type="text">
				</div>
			</div>
			<div class="form-group">
				<label class="col-sm-3 control-label">权利人产权证号：</label>
				<div class="col-sm-8">
					<input id="cqzh" name="cqzh" th:field="*{cqzh}" class="form-control" type="text">
				</div>
			</div>

		</div>
    </div>
    <div th:include="include::footer"></div>
    <script type="text/javascript">
		var prefix = ctx + "basp/filemanagerFile";
		$("#form-filemanagerFile-edit").validate({
			rules:{
				xxxx:{
					required:true,
				},
			},
			focusCleanup: true
		});
		
		function submitHandler() {
	        if ($.validate.form()) {
	            $.operate.save(prefix + "/edit", $('#form-filemanagerFile-edit').serialize());
	        }
	    }
	</script>
</body>
</html>
